
%table.table.table-condensed.dt-responsive
  %thead
    %tr
      %td.center Preference Id
      %td.center Date Created
      %td.center Date Modified
      %td.center Valid?
  %tbody
    -unless preference.nil?
      %tr
        %td.center= link_to("#{preference.id}",
                      user_preference_path(current_user, preference),
                      id: "preference_#{preference.id}", class: "btn btn-primary")
        %td.center #{preference.created_at}
        %td.center #{preference.updated_at}
        %td.center True
