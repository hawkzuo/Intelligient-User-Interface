%h1 Sample popular times for Business
%p From Monday to Sunday

.row
  .col-sm-12
    -@result_hash.each_slice(2) do |works|
      .row
        -works.each do |name, one_place_result|
          .col-sm-6
            %p Popular times for #{name}:
            -one_place_result.each do |day, bars|
              %p #{day}
              %p=column_chart(bars, download: true)
              %p=column_chart(eval("{7=>0, 8=>0, 9=>0, 10=>0, 11=>0, 12=>0, 13=>0, 14=>0, 15=>0, 16=>5, 17=>7, 18=>9, 19=>11, 20=>14, 21=>13, 22=>10, 23=>0, 0=>0}"), width: "1000px", height: "800px")

